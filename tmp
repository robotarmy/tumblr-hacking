<script>

 // post 
  var load_selector = function(selector,data)
  {
  	var html = "<div class='post'>";
  	for (var i=0; i < data['posts'].length; i++)  	
	{
	var post = data['posts'][i];
	var type = post['type'];
	var date = post['date'];
	var title = post[type+"-title"];
	var body =post[type+"-body"];
	var url = post['url'];
	
	html += "<div class='zoomable labels dim_box onegreen'>"+
          	 "<div class='date'><a href='"+url+"'>"+date+"</a></div>"+
            "</div>"+
			 "<div class='zoomable "+type+" dim_box onegreen'>"+
			 "<h2><a href='"+url+"'>"+ title +"</a></h2>"+
             "<div class='body'> " + body +" </div>"+
             "</div>";
    }
  	html += "  </div>";
  
	$(selector).html(html);

   }
  var load_random = function(data) {
	load_selector('#random',data);
	};
  var load_posts = function(data) {
	load_selector('#content .loaded',data);
    };
  scaleTextEm = function(size) {
  //  return this.each(function() { 
//	$(this);
	var to_size = size;
	var from_size = 69;	
	var end_size = 8;
	var start_size = 0;		
	if (from_size >= to_size)	{
		end = from_size ;
		start = to_size;
	}	else {
		 end = to_size	;		
		 start = from_size;
	}
		var current = start;
		var step_size = 10;
		var b;
		b = function() {
				current +=	step_size;
				$('.change').css({'font-size' : end-current+start+'em'});			
				
				if(current != end)
				{					
					/* over 1 billion people suffer from lack of clean water */
					// call b every 10 units
					setTimeout(b,10)
				}
			};
		b();
//	});
  }
	//JQuery
	$.fn.extend({
	});

	$(document).ready(function(){
	   scaleTextEm(8);
	
	});
</script>

